{
  "name": "kpclone",
  "version": "1.0.0",
  "description": "Backend service for clone of popular e-commerce service in Serbia called KupujemProdajem",
  "main": "index.js",
  "scripts": {
    "create-db": "npx sequelize-cli db:create",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "reset": "npx sequelize-cli db:migrate:undo:all",
    "start": "node ./index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stefi99R/KpClone.git"
  },
